{
  "manifest_version": 3,
  "name": "Roblox Server Region Helper",
  "version": "1.0",
  "description": "Enter a Roblox server JobId and fetch its region.",
  "permissions": ["scripting", "activeTab"],
  "host_permissions": [
    "https://www.roblox.com/*"
  ],
  "content_scripts": [
    {
      "matches": ["https://www.roblox.com/*"],
      "js": ["content.js"],
      "run_at": "document_idle"
    }
  ]
}
